{
  "extends": [
    "config:recommended",
    "group:allNonMajor",
    "schedule:weekly"
  ],
  "ignorePresets": [
    ":prHourlyLimit2"
  ],
  "rangeStrategy": "update-lockfile",
  "git-submodules": {
    "enabled": true
  },
  "packageRules": [
    {
      "matchCategories": [
        "js"
      ],
      "enabled": false
    },
    {
      "groupName": "git submodules",
      "groupSlug": "git-submodules",
      "automerge": true,
      "matchPackageNames": [
        "submodules/**"
      ]
    }
  ]
}
